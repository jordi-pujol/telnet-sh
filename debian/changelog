    _printf_printable() {
        #printf '%s' "${1}" | tr -cd '[:print:]\n\r\t\b\033'
        #printf '%s' "${1}" | cat -v | sed -re '\|\^M| s|||g'
        printf '%s' "${1}" | tr -cd '[ -~]\n\r\t\b\033'
    }

